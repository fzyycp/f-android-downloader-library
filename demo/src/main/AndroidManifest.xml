<?xml dbVersion="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.faury.downloaderdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:dbName=".MainActivity">
            <intent-filter>
                <action android:dbName="android.intent.action.MAIN" />

                <category android:dbName="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>